{
  "version": 2,
  "builds": [
    {
      "src": "serve/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "serve/dist/main.js"
    }
  ]
}